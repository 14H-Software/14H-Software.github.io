<Style 					x:Key="full-width--full-height--dot-pattern--grid"	TargetType="Grid"								>
	<Setter 			Property="HorizontalAlignment"						Value="Stretch"									/>
	<Setter 			Property="VerticalAlignment"						Value="Stretch"									/>
	<Setter 			Property="Background" 								Value="{flictl:TileBrush_4 'dotpattern'}"		/>
</Style																														>
<Style 					x:Key="full-width--full-height--dot-pattern--rect"	TargetType="Rectangle"							>
	<Setter 			Property="HorizontalAlignment"						Value="Stretch"									/>
	<Setter 			Property="VerticalAlignment"						Value="Stretch"									/>
	<Setter 			Property="Fill" 									Value="{flictl:TileBrush_4 'dotpattern'}"		/>
</Style																														>
<Style 					x:Key="full-width--full-height--hittest--grid"		TargetType="Grid"							>
	<Setter 			Property="HorizontalAlignment"						Value="Stretch"									/>
	<Setter 			Property="VerticalAlignment"						Value="Stretch"									/>
	<Setter 			Property="Background"								Value="#00000000"								/>
</Style																														>
<LinearGradientBrush 	x:Key="newsentry-animpanel"		StartPoint="0,0"	EndPoint="1,0"									>
	<GradientStop		Offset="0"											Color="#91FFFFFF"								/>
	<GradientStop		Offset="1"											Color="#00000000"								/>
</LinearGradientBrush>
<DropShadowEffect 		x:Key="newsentry-sidepanel-glow"	BlurRadius="4"	ShadowDepth="0"		Color="#FFFFFF"				/>
<SolidColorBrush		x:Key="logo-foreground"								Color="#FFFFFF"									/>
<SolidColorBrush		x:Key="logo-background"								Color="#1F1F1F"									/>
<LinearGradientBrush 	x:Key="silver_gr"		StartPoint="1,0" 			EndPoint="1,1"									>
	<GradientStop 		Offset="0.0" 										Color="#C3C3C3" 								/>
	<GradientStop 		Offset="1.0" 										Color="#818181"									/>
</LinearGradientBrush																										>
<LinearGradientBrush 	x:Key="defcol_gr"		StartPoint="1,0" 			EndPoint="1,1"									>
	<GradientStop 		Offset="0.0" 										Color="#3F454E" 								/>
	<GradientStop 		Offset="1.0" 										Color="#1F252E"									/>
</LinearGradientBrush																										>
